{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Capability for the main window",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "core:window:default",
        "core:webview:default",
        "core:event:default",
        "core:path:default",
        "core:app:default",
        "fs:default",
        "fs:allow-read-file",
        "fs:allow-read-text-file",
        "fs:allow-exists",
        "dialog:default",
        "dialog:allow-open",
        "shell:default"
    ]
}